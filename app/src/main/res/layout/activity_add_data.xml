<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="sgomez.ejercicios.apuntaextra.Activities.AÃ±adirDatos._AddDataActivity">

    <Button
        android:id="@+id/buttonAddExtra"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="buttonAddExtraOnClick"
        android:text="@string/addExtra" />

    <Button
        android:id="@+id/buttonAddLocal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="buttonAddLocalOnClick"
        android:text="@string/addLocal" />

    <Button
        android:id="@+id/buttonAddCamarero"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="buttonAddCamareroOnClick"
        android:text="@string/addCamarero" />

    <Button
        android:id="@+id/addCocina"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="buttonAddCocinaOnClick"
        android:text="@string/addCocina" />
</LinearLayout>
